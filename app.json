{
  "expo": {
    "name": "metromarket",
    "slug": "metromarket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoMetroMarket-completo.png",
    "scheme": "metromarket",
    "userInterfaceStyle": "automatic",
    "extra": {
      "eas": {
        "projectId": "999a2699-0463-4392-ac5a-643a7972b8b2"  
      }
    },
    "permissions": [
      "CAMERA",
      "MEDIA_LIBRARY"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoMetroMarket-completo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LogoMetroMarket-completo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
